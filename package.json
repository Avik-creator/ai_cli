{
  "name": "agentic-cli",
  "version": "1.0.0",
  "description": "An agentic CLI tool for web search, PR reviews, code generation, and command execution using Vercel AI SDK",
  "main": "src/index.js",
  "type": "module",
  "bin": {
    "agentic": "./src/main.js"
  },
  "scripts": {
    "start": "bun run src/main.js",
    "dev": "bun run --watch src/main.js",
    "cli": "bun run src/main.js",
    "chat": "bun run src/main.js chat",
    "setup": "bun run src/main.js config setup"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "vercel-ai-sdk",
    "ai-gateway",
    "exa",
    "pr-review",
    "code-generation"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.56",
    "@ai-sdk/google": "^2.0.46",
    "@ai-sdk/groq": "^2.0.33",
    "@ai-sdk/mistral": "^2.0.26",
    "@ai-sdk/openai": "^2.0.86",
    "@ai-sdk/xai": "^2.0.40",
    "@clack/prompts": "^0.11.0",
    "@exalabs/ai-sdk": "^1.0.5",
    "ai": "^5.0.89",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "dotenv": "^17.2.3",
    "figlet": "^1.9.3",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "node-fetch": "^3.3.2",
    "simple-git": "^3.27.0",
    "yocto-spinner": "^1.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}